{
    "name": "Displore/Blog",
    "type": "library",
    "description": "A blog built on Laravel.",
    "keywords": [
        "Displore",
        "Blog"
    ],
    "homepage": "https://github.com/Displore/Blog",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "JeroenG",
            "email": "jeroengjeroeng@gmail.com",
            "homepage": "https://github.com/Jeroen-G",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.5",
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Displore\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Displore\\Blog\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Displore\\Blog\\BlogServiceProvider"
            ]
        }
    }
}
